<%= f.input :id, readonly: true%>

<%= s.input :fecharec, 
  as: :string, 
  input_html: {
     "data-behaviour" => "datepicker",
    "data-toggle" => 'tooltip',
    "title" => 'Fecha inicial de atención al caso'
  }
%>

<% colus= current_usuario.oficina_id ? 
  Usuario.habilitados.where(oficina_id: current_usuario.oficina_id).
    order(:nombre) :
  Usuario.habilitados.order(:nombre) 
%>
<%= s.association :usuario,
  collection: colus,
  include_blank: false,
  label_method: :nusuario, 
  value_method: :id 
%>

<% colof= current_usuario.oficina_id ? 
  Sip::Oficina.habilitados.where(id: current_usuario.oficina_id) :
  Sip::Oficina.habilitados
%>
<%= s.association :oficina,
  collection: colof,
  include_blank: false,
  label_method: :nombre, 
  value_method: :id,
  input_html: {
    "data-toggle" => 'tooltip',
    "title" => 'Oficina del SJR que reporta el caso'
  }
%>

<%= f.input :fecha,
  as: :string, 
  wrapper_html: { style: "padding-bottom: 18px;" },
  input_html: {
    "data-behaviour" => "datepicker",
    "data-toggle" => 'tooltip',
    "title" => 'Fecha en la que el solicitante sufrió el desplazamiento principal'
  }
%>

<%= render partial: 'basicos_pie', locals: {f: f, s: s} %>

