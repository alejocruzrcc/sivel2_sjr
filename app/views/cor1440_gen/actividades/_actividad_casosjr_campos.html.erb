<tr class='nested-fields'>
  <td>
    <% 
      ph = 'Buscar No. Caso, contacto o identificacion del contacto'
      ac = { 
        #'data-autocompleta' => 'no' 
      }
    %>
    <%= f.input :casosjr_id,  placeholder: ph,
      label: false,
      as: :string,
      :input_html => ac 
    %>
  </td>
  <td>
    <span class="nombres">
      <%= f.object.casosjr.contacto.nombres if f.object && f.object.casosjr && f.object.casosjr.contacto %>
    </span>
  </td>
  <td>
    <span class="apellidos">
      <%= f.object.casosjr.contacto.apellidos if f.object && f.object.casosjr && f.object.casosjr.contacto %>
    </span>
  </td>
  <td>
    <span class="tipodocumento">
      <%= f.object.casosjr.contacto.tdocumento.sigla if f.object && f.object.casosjr && f.object.casosjr.contacto && f.object.casosjr.contacto.tdocumento %>
    </span>
  </td>
  <td>
    <span class="numerodocumento">
      <%= f.object.casosjr.contacto.numerodocumento if f.object && f.object.casosjr && f.object.casosjr.contacto %>
    </span>
  </td>
  <td>
    <%= f.input :id, as: :hidden %>
    <%= link_to_remove_association "Eliminar", f, 
      :class => 'btn-danger' 
    %>
    <div><%= f.error_notification %></div>
  </td>
</tr>
